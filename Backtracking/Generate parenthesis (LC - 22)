//FASTER APPROACH 
void solve(vector<string> &ans, int open, int close, string output)
    {
        if(open == 0 && close == 0)
            return ans.push_back(output);

        if(open > 0)
        {
            output.push_back('(');
            solve(ans, open - 1, close, output);
            output.pop_back();
        }
        if(close > open)
        {
            output.push_back(')');
            solve(ans, open, close - 1, output);
            output.pop_back();
        }
        return;
    }

    vector<string> generateParenthesis(int n) 
    {
        vector<string> ans;
        string output = "";
        int open = n;
        int close = n;
        solve(ans, open, close, output);
        return ans;    
    }

//SLIGHTLY SLOWER APPROACH
void solve(int open, int close, vector<string> &ans, string output)
    {
        if(open == 0 && close == 0)
            return ans.push_back(output);
        if(open > close || open < 0 || close < 0)
            return;

        solve(open - 1, close, ans, output + '(');
        solve(open, close - 1, ans, output + ')');
        
        return;
    }

    vector<string> generateParenthesis(int n) 
    {
        vector<string> ans;
        int open = n;
        int close = n;
        string output = "";
        
        solve(open, close, ans, output);
        
        return ans;    
    }
