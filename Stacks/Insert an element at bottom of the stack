void solve(stack<int> &St, int X)
    {
        if(St.empty())
        {
            St.push(X);
            return;
        }
        
        int temp = St.top();
        St.pop();
        
        solve(St, X);
        
        St.push(temp);
        return;
    }

    stack<int> insertAtBottom(stack<int> St,int X)
    {
       solve(St, X); 
       return St;
    }
